public class NotesSlide6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i=1;
		int j =10;
		int k = 30;
		
	    while (i<5){
	      System.out.print(i + " ");
	      i++;   
	    } // end of first while
	    System.out.println();
	    while(j<20) {
	    	System.out.print(j + " ");
	    	j = j +1;
	    	
	    } // end of second while
	    System.out.println();
	    while(k>10) {
	    	System.out.print(k + " ");
	    	k--;
	} // end main

}// end class
}
